<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.losteaka.no001.oscilloscopeapp">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.BLUETOOTH_PRIVILEGED" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:name=".SampleApplication"
        android:icon="@mipmap/ic_launcher2_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher2_foreground"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="landscape"
           >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".DeviceActivity"
            android:label="@string/activity_device"
            android:screenOrientation="landscape"
            android:windowSoftInputMode="adjustResize" />

        <activity
            android:name=".DeviceActivity2"
            android:label="@string/activity_device"
            android:screenOrientation="landscape"
            android:windowSoftInputMode="adjustResize" />

        <activity
            android:name=".BitmapActivity"
            android:label="@string/activity_device"
            android:screenOrientation="landscape"
            android:windowSoftInputMode="adjustResize" />

        <!--Activityを追加したときはここで追加する-->
        <activity
            android:name=".DeleteActivity"
            android:label="@string/activity_device"
            android:screenOrientation="landscape"
            android:windowSoftInputMode="adjustResize" />

        <activity
            android:name=".OpenActivity"
            android:label="@string/activity_device"
            android:screenOrientation="landscape"
            android:windowSoftInputMode="adjustResize" />
        <!--Activityを追加したときはここで追加する-->


        <activity
               android:theme="@style/Theme.AppCompat.Light.DarkActionBar"
               android:name="com.google.android.gms.oss.licenses.OssLicensesActivity"
               />
             <activity
               android:theme="@style/Theme.AppCompat.Light.DarkActionBar"
               android:name="com.google.android.gms.oss.licenses.OssLicensesMenuActivity"
               />







    </application>

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

</manifest>